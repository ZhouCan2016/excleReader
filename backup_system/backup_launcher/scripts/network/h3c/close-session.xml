         <auth:AuthInfo>%authinfo%</auth:AuthInfo>
      </auth:Authentication>
   </env:Header>
   <env:Body>
	<rpc message-id="101" xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
		<close-session/>
	</rpc>
   </env:Body>
</env:Envelope>
